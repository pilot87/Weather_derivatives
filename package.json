{
  "name": "forecast_trading",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "scripts": {
    "server:dev": "ts-node-dev --respawn --transpile-only ./src/app.ts",
    "server:prod": "tsc && node ./build/app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server:dev\" \"npm run client\"",
    "start": "cross-env NODE_ENV=production tsc && node build/app.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "express-validator": "^6.8.1",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.19",
    "path": "^0.12.7",
    "node-schedule": "^1.3.2",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.16",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
